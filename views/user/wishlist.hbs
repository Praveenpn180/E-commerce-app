<!-- Shoping Cart -->
{{#if wcount}}
	<div class="bg0 p-t-75 p-b-85">
		<div class="container">
			<div class="row">
				<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
					<div class=" m-r--38 m-lr-0-xl">
						<div class="wrap-table-shopping-cart">
							<table class="table-shopping-cart">
								<tr class="table_head">
									<th class="column-1 col-2">Product</th>
									<th class="column-2 col-2">Item</th>
									<th class="column-3 col-2">Price</th>
									<th class="column-3 col-2"></th>
									
								</tr>
                            {{#each wishlist}}
								<tr class="table_row">
									<td class="column-1">
										<div class="how-itemcart1">
											<img src="/product-images/{{productdetails._id}}.jpg" alt="IMG">
										</div>
									</td>
									<td class="column-2">{{productdetails.Name}}</td>
									<td class="column-3">{{productdetails.Price}}</td>
									
									
                                    <td class="column-6"> <button onclick="moveToCart('{{productdetails._id}}')" class="btn btn-dark">
							Move to cart
						</button></td>
						 <td class="column-6"> <button onclick="remove('{{productdetails._id}}')" class="btn btn-outline-danger ml-3">
							x
						</button></td>
								</tr>
                               
                                {{/each}}
								
							</table>
						</div>

						
					</div>
				</div>
               

				
			</div>
		</div>
	</div>
	{{else}}
<div class="container">
	<div class="row flex justify-content-around">
		<div class="col-lg-10 col-xl-6 m-lr-auto m-b-50 " style="margin-top:5rem ;">
			<img src="/userAssets/images/emptywishlist.jpg" alt="">


		</div>

	</div>

</div>
	{{/if}}


    